<Window x:Class="IT481_Fitzpatrick_Unit_3.frmLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IT481_Fitzpatrick_Unit_3"
        mc:Ignorable="d"
        Title="frmLogin" Height="404" Width="499">
    <StackPanel>
        <TextBlock Name="lblStatus">Enter Login Credentials to login to SQL Server</TextBlock>
        <TextBlock>Username</TextBlock>
        <TextBox Name="txtUsername"></TextBox>
        <TextBlock>Password</TextBlock>
        <TextBox Name="txtPassword"></TextBox>
        <Button Name="btnLogin" Click="btnLogin_Click_1">Login</Button>
    </StackPanel>
</Window>
